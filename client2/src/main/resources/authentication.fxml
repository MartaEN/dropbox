<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SignInScreen">
   <children>
      <Label layoutX="32.0" layoutY="41.0" text="Вход для зарегистрированных пользователей">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="32.0" layoutY="60.0" text="test run:  try login1 + pass, login2 + pass" textFill="RED" />
      <TextField fx:id="loginField" layoutX="169.0" layoutY="91.0" prefHeight="25.0" prefWidth="181.0" />
      <PasswordField fx:id="passwordField" layoutX="169.0" layoutY="136.0" onAction="#signIn" prefHeight="25.0" prefWidth="181.0" />
      <Label layoutX="32.0" layoutY="95.0" prefHeight="17.0" prefWidth="149.0" text="Имя пользователя:" />
      <Label layoutX="32.0" layoutY="140.0" prefHeight="17.0" prefWidth="123.0" text="Пароль:" />
      <Button layoutX="298.0" layoutY="188.0" mnemonicParsing="false" onAction="#signIn" text="Войти" />
      <Hyperlink layoutX="195.0" layoutY="225.0" onAction="#switchToRegistration" text="Ещё не зарегистрированы?" />
   </children>
</AnchorPane>
